/**
 * Codígo de veridicação de idade e se a pessoa pode ou não doar sangue
 *  IN E RANGE 
 */
const val MIN_AGE =16
const val MAX_AGE = 68
fun main() {
var age = (10..100).random() // escolhe um número aleatorio 
println(age) // exibi o número
println(age in MIN_AGE.. MAX_AGE)// AGE ESTA ENTRE AS IDADES max e min
println (age >= MIN_AGE && age <= MAX_AGE) //AGE É 
}
